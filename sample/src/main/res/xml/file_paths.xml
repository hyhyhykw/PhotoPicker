<?xml version="1.0" encoding="utf-8"?>

<!--suppress AndroidDomInspection -->
<paths>

    <!--代表app 私有的存储区域 Context.getCacheDir()目录下的images目录 -->
    <!-- /data/user/0/co.zanq.app/cache-->
    <cache-path
        name="picker_private_cache"
        path=""/>

    <files-path
        name="picker_private_file"
        path=""/>
    <!--代表app 外部存储区域根目录下的文件 Context.getExternalCacheDir目录下的images目录-->
    <!--/storage/emulated/0/Android/data/co.zanq.app/cache-->
    <external-cache-path
        name="picker_external_cache"
        path=""/>

    <external-files-path
        name="picker_external_file"
        path=""/>

    <external-path
        name="external_storage_root"
        path="."/>

    <external-path
        name="files_root"
        path="Android/data/com.hy.photopicker/"/>
</paths>